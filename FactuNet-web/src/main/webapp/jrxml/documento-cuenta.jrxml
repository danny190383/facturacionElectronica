	<queryString>
		<![CDATA[SELECT cuenta_factura.codigo,mantenimiento.nombre, cuenta_factura.valor, cuenta_factura.saldo, cuenta_factura.fecha_vencimiento, cuenta_factura.numero, 
cuenta_factura.detalle
from cuenta_factura inner join forma_pago on
forma_pago.codigo = cuenta_factura.forma_pago inner join mantenimiento on
mantenimiento.codigo = forma_pago.codigo 
where cuenta_factura.factura_pago = $P{cuenta} ORDER BY cuenta_factura.fecha_vencimiento]]>
	</queryString>
	<field name="codigo" class="java.lang.Integer">
		<fieldDescription><![CDATA[Numero entero que indica la cuenta por cobrar a la que se esta realizando el abono o pago.]]></fieldDescription>
	</field>
	<field name="nombre" class="java.lang.String">
		<fieldDescription><![CDATA[Nombre de la bodega.]]></fieldDescription>
	</field>
	<field name="valor" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[el valor de pago o abono a la cta. por cobrar.]]></fieldDescription>
	</field>
	<field name="saldo" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[El saldo pendiente de cancelar de la cta por cobrar, ya que pueden hacer abonos parciales a la cta por cobrar.]]></fieldDescription>
	</field>
	<field name="fecha_vencimiento" class="java.sql.Date">
		<fieldDescription><![CDATA[Fecha en la que vence el pago de la cta, por cobrar.]]></fieldDescription>
	</field>
	<field name="numero" class="java.lang.String"/>
	<field name="detalle" class="java.lang.String">
